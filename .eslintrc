{
  "extends": ["airbnb", "plugin:flowtype/recommended"],
  "parser": "babel-eslint",
  "plugins": ["flowtype"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off"
  },
  "globals": {
    "document": true,
    "fetch": true,
    "Headers": true
  },
  "env": {
    "jest": true
  }
}
