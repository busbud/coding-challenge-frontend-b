{
  "extends": ["next", "airbnb", "prettier"],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "react/jsx-filename-extension": "off",
    "react/react-in-jsx-scope": "off",
    "react/function-component-definition": "off",
    "react/jsx-no-constructed-context-values": "off",
    "no-shadow": "off",
    "no-unused-vars": "off",
    "import/extensions": "off"
  }
}
