{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "10"
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ],
  "only": [
    "./**/*.js",
    "node_modules/jest-runtime"
  ]
}