@import 'uiAssets/vars.pcss';

.__SCOPE {
  > .tickets {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 auto;
    padding: 25px 0;
    width: 100%;
    max-width: 740px;

    @media (min-width: 700px) {
      flex-direction: row;
      align-items: flex-start;
      justify-content: space-around;
    }
  }
}
