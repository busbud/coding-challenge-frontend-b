{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",  
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "compileOnSave": true,
  "include": ["src/**/*"]
}
