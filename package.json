{
  "name": "initial",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "react": "16.8.3",
    "react-redux": "6.0.1",
    "react-scripts": "2.1.5",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "husky": "1.3.1",
    "jest-dom": "3.1.2",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "react-dom": "16.8.3",
    "react-testing-library": "6.0.0"
  }
}
