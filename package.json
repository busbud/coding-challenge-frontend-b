{
  "name": "coding-challenge-frontend-b",
  "version": "0.0.1",
  "description": "An ultimate Busbud coding challenge",
  "license": "MIT",
  "author": "Victor Musienko",
  "scripts": {
    "postinstall": "npm rebuild node-sass && npm run webpack",
    "lint-es": "eslint *.js src/* src/**/*",
    "lint-sass": "sass-lint -q -v",
    "lint": "npm run lint-es && npm run lint-sass",
    "start-dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start": "node ./server.js",
    "debug": "node $NODE_DEBUG_OPTION ./node_modules/.bin/webpack-dev-server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "github:sdwvit/coding-challenge-frontend-b"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "^20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.23.0",
    "compression-webpack-plugin": "0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "0.27.3",
    "eslint": "3.4.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.2.0",
    "file-loader": "0.10.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "19.0.2",
    "node-sass": "4.5.3",
    "react-test-renderer": "^15.6.1",
    "resolve-url-loader": "2.0.2",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.3",
    "style-loader": "0.14.0",
    "uglifyjs-webpack-plugin": "0.4.3",
    "url-loader": "0.5.8",
    "prop-types": "^15.5.10",
    "webpack": "3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "classnames": "^2.2.5",
    "express": "^4.15.4",
    "express-static-gzip": "^0.3.0",
    "i18next": "^9.0.0",
    "i18next-browser-languagedetector": "^2.0.0",
    "i18next-xhr-backend": "^1.4.2",
    "lodash.clonedeep": "4.5.0",
    "react": "^15.6.1",
    "react-bootstrap": "0.30.7",
    "react-dom": "15.4.2",
    "react-i18next": "^5.2.0",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-logger": "2.7.4",
    "redux-thunk": "2.1.0",
    "urijs": "^1.18.12",
    "whatwg-fetch": "2.0.3"
  },
  "engines": {
    "node": ">=v7.4.0",
    "npm": ">=5.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "roots": [
      "src",
      "__mocks__"
    ],
    "testRegex": "&*test\\.js$",
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/test/style.js"
    }
  }
}
