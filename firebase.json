{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/app/index.html"
    }, {
      "source": "/fr{,/**}",
      "destination": "/app/fr/index.html"
    } ]
  }
}
